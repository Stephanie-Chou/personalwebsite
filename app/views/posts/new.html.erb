<form accept-charset="UTF-8" action="/create" method="post">
  <div class="form-group">
    <textarea class="form-control" id="date" name="date" type="text" placeholder="Date"></textarea>
  </div>
  <div class="form-group">
    <textarea class="form-control" id="title" name="title" type="text" placeholder = "title"></textarea>
  </div>
  <div class="form-group">
    <textarea class="form-control" id="problem" name="problem" type="text" placeholder = "problem"></textarea>
  </div>
  <div class="form-group">
    <textarea class="form-control" id="language" name="language" type="text" placeholder = "language"></textarea>
  </div>
  <div class="form-group">
    <textarea class="form-control" id="runtime" name="runtime" type="text" placeholder = "runtime"></textarea>
  </div>
  <div class="form-group">
    <textarea class="form-control" id="content" name="content" type="text" placeholder = "content" rows = "10"></textarea>
  </div>
  <div class="form-group">
    <textarea class="form-control" id="code" name="code" type="text" placeholder = "code" rows = "20"></textarea>
  </div>
  <input class="btn btn-default" name="commit" type="submit" value="Log In">
</form>